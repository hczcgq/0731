<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:background="#f2f2f2"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@android:color/white"
            android:orientation="vertical"
            android:padding="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="姓名："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealNameTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="chenguoquan"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="性别："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealSexTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="男"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="出生日期："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealBirthTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1990-10－12"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="年龄："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealAgeTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="25"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="户号："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealRoonNoTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1234568"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="联系方式："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealContractTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="15926395578"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="区："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealRegionTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="姑苏区"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="街道："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealAreaTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="xx街道"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="社区："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealPropTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="xx社区"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="户籍地址："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealLocationTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="xx省xx市xx县"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@android:color/white"
            android:orientation="vertical"
            android:padding="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="转出时间："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealTurnOutTimeTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="chenguoquan"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="转出原因："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealTurnOutReasonTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="男"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="接收街道："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealReceiveRoadTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1990-10－12"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:text="接收社区："
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />

                <TextView
                    android:id="@+id/UndealReveiveAreaTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="25"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/common_text_size" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
